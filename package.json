{
  "name": "launcher",
  "description": "cool launcher",
  "version": "0.0.18",
  "license": "MIT",
  "author": "mighty",
  "main": "electron/main.js",
  "scripts": {
    "autobuild": "rollup -c -w",
    "dev": "concurrently \"npm run autobuild\" \"cross-env NODE_ENV=development ELECTRON_RELOAD=yes electron .\"",
    "prod": "cross-env NODE_ENV=production electron .",
    "dev-web": "sapper dev",
    "build": "sapper build",
    "releasewin": "cmd /c release.sh",
    "start": "electron-forge start",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "import": "yarn electron-forge import",
    "package": "electron-forge package",
    "setup": "",
    "export": "./electron/public",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "lowdb": "^1.0.0",
    "node-cmd": "^3.0.0",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "bulma": "^0.7.5",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "electron": "^11.0.4",
    "faunadb": "^2.9.4",
    "lowdb": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.0.3",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.16",
    "sirv": "^0.4.2",
    "svelte": "^3.31.0",
    "wait-on": "^3.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "mightyjol",
              "name": "launcher"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "template"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
